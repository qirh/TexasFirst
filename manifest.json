{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "name": "Texas First",
  "description": "In picker menus, Texas will load, in the place that only she deserves, at very top, #1",
  "version": "0.8.9",
  "author": "Saleh Alghusson",
  "offline_enabled": true,
  "homepage_url": "http://www.github.com/qirh/TexasFirst",
  "browser_action": {
    "default_title": "Â¡Tejas!"
  },
  "icons": {
  "16": "icons/icon-16.png",
  "48": "icons/icon-48.png",
  "128": "icons/icon-128.png"
  },
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
          "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/TexasFirst.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ]
}
