{
  "manifest_version": 2,
  "name": "Texas First",
  "description": "In picker menus, Texas will load, in the place that only she deserves, at very top, #1",
  "version": "0.8",
  "author": "Saleh Alghusson",
  "offline_enabled": true,
  "homepage_url": "http://www.github.com/qirh/TexasFirst",

  "browser_action": {
    "default_icon": "icon-white.png",
    "default_title": "meh here!"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["TexasFirst.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "offline_enabled": true,
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  }
}
